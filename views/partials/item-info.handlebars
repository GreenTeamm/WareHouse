<div class="card" style="visibility: visible; display: block;">

    <div class="card-header">
        <h4 class="item-names">{{name}}</h4>
        <button id="deleteButton" class="deleteBtn">X</button>
    </div>

    <div class="item-attributes">
        <ul>
            <li>{{material}}</li>
            <li>{{color}}</li>
            <li>${{price}}</li>
            <li>Id: {{id}}</li>
        </ul>

    </div>
</div>
